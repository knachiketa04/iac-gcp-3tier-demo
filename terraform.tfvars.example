# Example terraform.tfvars file
# Copy this to terraform.tfvars and customize the values

project_id       = "your-gcp-project-id"
region          = "us-central1"
environment     = "dev"
app_name        = "webapp-example"                    # Must start with lowercase letter, no underscores
gcs_bucket_name = "your-terraform-state-bucket"
db_user         = "postgres"
db_password     = "your-secure-password"
